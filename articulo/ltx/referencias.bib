month = nov,
number = {17},
pages = {7928--40},
pmid = {18406612},
title = {{Optimisation of the anaerobic digestion of agricultural resources.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18406612},
volume = {99},
year = {2008}
